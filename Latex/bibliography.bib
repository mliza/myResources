   issn = {0022-1120},
   journal = {Journal of Fluid Mechanics},
   month = {6},
   pages = {1-29},
   title = {Experimental study of the fine-scale structure of conserved scalar mixing in turbulent shear flows. Part 2. Sc $\approx$1},
